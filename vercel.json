{
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/landing.html"
    },
    {
      "source": "/app",
      "destination": "/public/index.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/public/$1"
    }
  ],
  "functions": {
    "api/generate-image.js": {
      "maxDuration": 60
    }
  }
}
