OPENROUTER_API_KEY=your_api_key_here
PORT=3000

# Image Generation Provider: "openrouter" or "replicate"
IMAGE_PROVIDER=openrouter

# Replicate (optional - only needed if IMAGE_PROVIDER=replicate)
REPLICATE_API_KEY=your_replicate_api_key
REPLICATE_MODEL=prunaai/z-image-turbo

# Supabase (optional - falls back to in-memory storage if not set)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
